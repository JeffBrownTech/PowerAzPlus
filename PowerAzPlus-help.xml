        <dev:remarks>
          <maml:para>Exports all secrets from "MyKeyVault" and saves them in the current working directory.</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 2 --------------------------</maml:title>
        <dev:code>PS C:\&gt; Export-AllKeyVaultSecrets -VaultName "MyKeyVault" -OutputPath "C:\Backup\Secrets"</dev:code>
        <dev:remarks>
          <maml:para>Exports all secrets from "MyKeyVault" and saves them in the specified directory.</maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>
